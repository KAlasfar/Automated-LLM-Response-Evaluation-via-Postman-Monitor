	"item": [
		{
			"name": "Test1_CapitalCity",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let result = pm.response.json();\r",
							"\r",
							"// Error handling for primary model\r",
							"if (!result.choices || !result.choices[0]) {\r",
							"    console.log(\"Primary model returned invalid response:\", result);\r",
							"    return pm.test(\"Model Response\", () => {\r",
							"        pm.expect.fail(\"Primary model returned invalid response\");\r",
							"    });\r",
							"}\r",
							"\r",
							"let answer = result.choices[0].message.content;\r",
							"let prompt = \"Which city is the capital of the country where the Eiffel Tower is located?\";\r",
							"\r",
							"// LLM evaluator\r",
							"pm.sendRequest({\r",
							"    url: \"https://openrouter.ai/api/v1/chat/completions\",\r",
							"    method: \"POST\",\r",
							"    header: {\r",
							"        \"Authorization\": `Bearer ${pm.environment.get(\"OPENROUTER_API_KEY\")}`,\r",
							"        \"Content-Type\": \"application/json\"\r",
							"    },\r",
							"    body: {\r",
							"        mode: \"raw\",\r",
							"        raw: JSON.stringify({\r",
							"            model: \"openrouter/bert-nebulon-alpha\",\r",
							"            messages: [\r",
							"                { role: \"system\", content: \"Reply ONLY with CORRECT or WRONG.\" },\r",
							"                { role: \"user\", content: `Prompt: ${prompt}\\nAnswer: ${answer}` }\r",
							"            ]\r",
							"        })\r",
							"    }\r",
							"}, (err, res) => {\r",
							"\r",
							"    // Error handling evaluator\r",
							"    if (err) {\r",
							"        console.log(\"Evaluator Error:\", err);\r",
							"        return pm.test(\"Evaluation Result\", () => {\r",
							"            pm.expect.fail(\"Evaluator failed due to API error\");\r",
							"        });\r",
							"    }\r",
							"\r",
							"    if (!res.json() || !res.json().choices) {\r",
							"        console.log(\"Evaluator invalid response:\", res);\r",
							"        return pm.test(\"Evaluation Result\", () => {\r",
							"            pm.expect.fail(\"Evaluator returned invalid response\");\r",
							"        });\r",
							"    }\r",
							"\r",
							"    let judge = res.json().choices[0].message.content.trim();\r",
							"\r",
							"    console.log(\"Prompt:\", prompt);\r",
							"    console.log(\"Answer:\", answer);\r",
							"    console.log(\"Evaluation:\", judge);\r",
							"\r",
							"    pm.test(\"Evaluation Result\", function () {\r",
							"        pm.expect(judge).to.be.oneOf([\"CORRECT\", \"WRONG\"]);\r",
							"    });\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
